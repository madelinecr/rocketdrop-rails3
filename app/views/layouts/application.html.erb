<!DOCTYPE html>
<html>
<head>
  <title>Rocketdrop</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if flash[:notice] %>
    <%= flash[:notice] %>
  <% end %>
  <%= flash[:error] %>

  <%= yield %>

  <%= debug(params) if Rails.env == "development" %>
</body>
</html>
